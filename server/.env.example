PORT=3001
NODE_ENV=development

# PayPal Configuration
# Get these from https://developer.paypal.com/dashboard/
PAYPAL_CLIENT_ID=your_sandbox_client_id_here
PAYPAL_CLIENT_SECRET=your_sandbox_client_secret_here
PAYPAL_MODE=sandbox

# Application Settings
PAYMENT_AMOUNT=249.95
CURRENCY=USD

# Session Configuration
SESSION_SECRET=change_this_to_a_random_string_in_production

# Frontend URL (for CORS)
FRONTEND_URL=https://kps.s3.ninja

# Webhook Configuration (optional - for IPN)
PAYPAL_WEBHOOK_ID=your_webhook_id_here
